<RullesGrid@Widget>
    BoxLayout:
        size: root.size
        padding: [10,10,10,30]
        orientation: 'vertical'
        spacing: 3
        AboutLable:
            size_hint: 1,0.2
            color: 0,0,0,1
            font_size: 16
            text: "About Memory battle"
        AboutLable1:
            text_size: self.size
            valign: 'top'
            halign: 'center'
            pos_hint: {'x': 0.01, 'y': 0}
            size_hint: 0.98,0.80
            color: 0,0,0,1
            font_size: 11
            text: "Memory battle is a mobile game that can be played with your friends. The person who collects most points wins the game.Your game results will be uploaded to the website so you can check your review your progress. Game allows a practice mode so you can improve your skills"
        AboutLable:
            size_hint: 1,0.18
            color: 0,0,0,1
            font_size: 16
            text: 'How to play ?'
        AboutLable1:
            pos_hint: {'x': 0.01, 'y': 0}
            size_hint: 0.98,0.95
            color: 0,0,0,1
            text_size: self.size
            valign: 'top'
            halign: 'center'
            font_size: 11
            text: "The game allowes 4x4 and 5x6 fields of memory puzzles that you have to uncover in pairs. When selecting the number of players and difficulty you are ready to play. The profile owner always starts first. Points are granted for correct pair and helf of a point is taken for failed attempts. Pictures in 4x4 field will be shown for 2 seconds and 3 seconds in 5x6 field"
    AnchorLayout:
        size: root.size
        padding: [50,10,50,0]
        anchor_y: "bottom"
        anchor_x: "center"
        EditedButton:
            size_hint: .5,.07
            text:"Back"
            bold: True
            on_press: self.back_color=(0,0,0,1)
            on_release:
                app.root.current = "main"
                app.root.transition.direction = 'right'
                self.back_color=(0.2,0.9,0.9,1)

